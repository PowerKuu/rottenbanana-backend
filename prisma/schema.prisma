generator client {
  provider = "prisma-client"
  output   = "./generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model PrefrenceTag {
  id String @id @default(cuid())
  tag String

}

model Vendor {
  id String @id @default(cuid())
  name String
}

model Item {
  id String @id @default(cuid())
  name String
}

model Outfit {
  id String @id @default(cuid())

}

model TryOnOutfit {
  id String @id @default(cuid())
}

model Post {
  id String @id @default(cuid())
}